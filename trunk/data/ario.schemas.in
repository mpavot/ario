<gconfschemafile>
   <schemalist>
      <schema>
        <key>/schemas/apps/ario/state/vpaned_position</key>
        <applyto>/apps/ario/state/vpaned_position</applyto>
        <owner>ario</owner>
        <type>int</type>
        <default>400</default>
        <locale name="C">
        <short>Position of main window pane</short>
        <long>Position of main window pane</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/ario/state/playlists_hpaned_position</key>
        <applyto>/apps/ario/state/playlists_hpaned_position</applyto>
        <owner>ario</owner>
        <type>int</type>
        <default>250</default>
        <locale name="C">
        <short>Position of playlists pane</short>
        <long>Position of playlists pane</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/ario/state/filesystem_hpaned_position</key>
        <applyto>/apps/ario/state/filesystem_hpaned_position</applyto>
        <owner>ario</owner>
        <type>int</type>
        <default>250</default>
        <locale name="C">
        <short>Position of filesystem pane</short>
        <long>Position of filesystem pane</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/ario/state/statusbar_hidden</key>
        <applyto>/apps/ario/state/statusbar_hidden</applyto>
        <owner>ario</owner>
        <type>bool</type>
        <default>FALSE</default>
        <locale name="C">
        <short>Statusbar visibility</short>
        <long>If true, the statusbar is hidden.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/ario/host</key>
        <applyto>/apps/ario/host</applyto>
        <owner>ario</owner>
        <type>string</type>
        <default>localhost</default>
        <locale name="C">
        <short>Host to connect to</short>
        <long>Host to connect to</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/ario/port</key>
        <applyto>/apps/ario/port</applyto>
        <owner>ario</owner>
        <type>int</type>
        <default>6600</default>
        <locale name="C">
        <short>Port to connect to</short>
        <long>Port to connect to</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/ario/timeout</key>
        <applyto>/apps/ario/timeout</applyto>
        <owner>ario</owner>
        <type>float</type>
        <default>8.0</default>
        <locale name="C">
        <short>The timeout for the connection to mpd</short>
        <long>The timeout for the connection to mpd</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/ario/autoconnect</key>
        <applyto>/apps/ario/autoconnect</applyto>
        <owner>ario</owner>
        <type>bool</type>
        <default>1</default>
        <locale name="C">
        <short>Define if Ario must autoconnect to mpd or not</short>
        <long>Define if Ario must autoconnect to mpd or not</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/ario/password</key>
        <applyto>/apps/ario/password</applyto>
        <owner>ario</owner>
        <type>string</type>
        <default></default>
        <locale name="C">
        <short>The password to connect to mpd</short>
        <long>The password to connect to mpd</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/ario/cover_tree_hidden</key>
        <applyto>/apps/ario/cover_tree_hidden</applyto>
        <owner>ario</owner>
        <type>bool</type>
        <default>0</default>
        <locale name="C">
        <short>Hide the covers in the albums treeview</short>
        <long>When enabled, ario hide the covers in the albums treeview</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/ario/cover_amazon_country</key>
        <applyto>/apps/ario/cover_amazon_country</applyto>
        <owner>ario</owner>
        <type>string</type>
        <default>com</default>
        <locale name="C">
        <short>The amazon country to use to get the covers</short>
        <long>You can search for your cover on amazon.com .fr .de .uk or .ca to have more accurate results</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/ario/state/window_width</key>
        <applyto>/apps/ario/state/window_width</applyto>
        <owner>ario</owner>
        <type>int</type>
        <default>600</default>
        <locale name="C">
        <short>Main window width</short>
        <long>Main window width</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/ario/state/window_height</key>
        <applyto>/apps/ario/state/window_height</applyto>
        <owner>ario</owner>
        <type>int</type>
        <default>600</default>
        <locale name="C">
        <short>Main window height</short>
        <long>Main window height</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/ario/state/window_maximized</key>
        <applyto>/apps/ario/state/window_maximized</applyto>
        <owner>ario</owner>
        <type>bool</type>
        <default>1</default>
        <locale name="C">
        <short>Main window maximization state</short>
        <long>If true, the main window is maximized.</long>
        </locale>
      </schema>
      
      <schema>
        <key>/schemas/apps/ario/use_proxy</key>
        <applyto>/apps/ario/use_proxy</applyto>
        <owner>ario</owner>
        <type>bool</type>
        <default>0</default>
        <locale name="C">
        <short>Define if Ario must use a proxy for remote connections</short>
        <long>Define if Ario must use a proxy for remote connections</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/ario/proxy_address</key>
        <applyto>/apps/ario/proxy_address</applyto>
        <owner>ario</owner>
        <type>string</type>
        <default>192.168.0.1</default>
        <locale name="C">
        <short>Define the address of the proxy</short>
        <long>Define the address of the proxy</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/ario/music-dir</key>
        <applyto>/apps/ario/music-dir</applyto>
        <owner>ario</owner>
        <type>string</type>
        <default></default>
        <locale name="C">
        <short>Define the path to the music</short>
        <long>Define the path to the music</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/ario/proxy_port</key>
        <applyto>/apps/ario/proxy_port</applyto>
        <owner>ario</owner>
        <type>int</type>
        <default>8080</default>
        <locale name="C">
        <short>Define the port of the proxy</short>
        <long>Define the port of the proxy</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/ario/state/source</key>
        <applyto>/apps/ario/state/source</applyto>
        <owner>ario</owner>
        <type>int</type>
        <default>0</default>
        <locale name="C">
        <short>Define the source used in Ario</short>
        <long>0 for the library, 1 for the radios, etc...</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/ario/show_tabs</key>
        <applyto>/apps/ario/show_tabs</applyto>
        <owner>ario</owner>
        <type>bool</type>
        <default>1</default>
        <locale name="C">
        <short>Define if the view tabs are shown.</short>
        <long>Define if the view tabs are shown.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/ario/trayicon_behavior</key>
        <applyto>/apps/ario/trayicon_behavior</applyto>
        <owner>ario</owner>
        <type>int</type>
        <default>0</default>
        <locale name="C">
        <short>Define the behavior of a double click on the tray icon.</short>
        <long>0 for 'play/pause', 1 for 'play next song', 2 for 'do nothing'.</long>
        </locale>
      </schema>


      <schema>
        <key>/schemas/apps/ario/state/track_column_size</key>
        <applyto>/apps/ario/state/track_column_size</applyto>
        <owner>ario</owner>
        <type>int</type>
        <default>50</default>
        <locale name="C">
        <short>Define the size of the track column.</short>
        <long>Define the size of the track column.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/ario/state/title_column_size</key>
        <applyto>/apps/ario/state/title_column_size</applyto>
        <owner>ario</owner>
        <type>int</type>
        <default>200</default>
        <locale name="C">
        <short>Define the size of the title column.</short>
        <long>Define the size of the title column.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/ario/state/artist_column_size</key>
        <applyto>/apps/ario/state/artist_column_size</applyto>
        <owner>ario</owner>
        <type>int</type>
        <default>200</default>
        <locale name="C">
        <short>Define the size of the artist column.</short>
        <long>Define the size of the artist column.</long>
        </locale>
      </schema>


      <schema>
        <key>/schemas/apps/ario/state/album_column_size</key>
        <applyto>/apps/ario/state/album_column_size</applyto>
        <owner>ario</owner>
        <type>int</type>
        <default>200</default>
        <locale name="C">
        <short>Define the size of the album column.</short>
        <long>Define the size of the album column.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/ario/first_time_flag</key>
        <applyto>/apps/ario/first_time_flag</applyto>
        <owner>ario</owner>
        <type>bool</type>
        <default>FALSE</default>
        <locale name="C">
        <short>True if the initial assistant has already been run.</short>
        <long>True if the initial assistant has already been run.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/ario/plugins/active-plugins</key>
        <applyto>/apps/ario/plugins/active-plugins</applyto>
        <owner>ario</owner>
        <type>list</type>
        <list_type>string</list_type>
        <default>[filesystem,radios,wikipedia]</default>
        <locale name="C">
	<short>Active plugins</short>
	<long>List of active plugins. It contains the "Location" of the active plugins. See the .ario-plugin file for obtaining
        the "Location" of a given plugin.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/ario/state/sources-order</key>
        <applyto>/apps/ario/state/sources-order</applyto>
        <owner>ario</owner>
        <type>list</type>
        <list_type>string</list_type>
        <default>[]</default>
        <locale name="C">
	<short>Sources order</short>
	<long>Ordered list of sources.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/ario/cover-providers</key>
        <applyto>/apps/ario/cover-providers</applyto>
        <owner>ario</owner>
        <type>list</type>
        <list_type>string</list_type>
        <default>[local,amazon,lastfm]</default>
        <locale name="C">
	<short>Covers Sources</short>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/ario/active-cover-providers</key>
        <applyto>/apps/ario/active-cover-providers</applyto>
        <owner>ario</owner>
        <type>list</type>
        <list_type>string</list_type>
        <default>[local,amazon,lastfm]</default>
        <locale name="C">
	<short>Active covers sources</short>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/ario/lyrics-providers</key>
        <applyto>/apps/ario/lyrics-providers</applyto>
        <owner>ario</owner>
        <type>list</type>
        <list_type>string</list_type>
        <default>[lyricwiki,leoslyrics]</default>
        <locale name="C">
	<short>Lyrics Sources</short>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/ario/active-lyrics-providers</key>
        <applyto>/apps/ario/active-lyrics-providers</applyto>
        <owner>ario</owner>
        <type>list</type>
        <list_type>string</list_type>
        <default>[lyricwiki,leoslyrics]</default>
        <locale name="C">
	<short>Active lyrics sources</short>
        </locale>
      </schema>

    </schemalist>
</gconfschemafile>
