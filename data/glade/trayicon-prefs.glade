                            <property name="draw_indicator">True</property>
                            <signal name="toggled" handler="ario_trayicon_preferences_notification_check_changed_cb"/>
                          </widget>
                        </child>
                      </widget>
                    </child>
                    <child>
                      <widget class="GtkHBox" id="hbox4">
                        <property name="visible">True</property>
